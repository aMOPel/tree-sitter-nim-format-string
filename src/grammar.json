{
  "name": "nim_format_string",
  "rules": {
    "source_file": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "string_content"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "string_content": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "matching_curlies"
          },
          {
            "type": "SYMBOL",
            "name": "_everything_else"
          }
        ]
      }
    },
    "_everything_else": {
      "type": "PATTERN",
      "value": "[^\\n\\r\\s]"
    },
    "matching_curlies": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "_everything_else"
            }
          },
          "named": true,
          "value": "everything_else_inside"
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[\\n\\r\\s]+"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

